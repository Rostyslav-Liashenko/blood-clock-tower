generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(uuid())
  firstName String @map("first_name") @db.VarChar(250)
  lastName  String @map("last_name") @db.VarChar(250)

  @@map("user")
}
